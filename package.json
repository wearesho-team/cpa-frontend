{
  "name": "@wearesho/cpa-integration-frontend",
  "version": "1.0.0-dev",
  "description": "CPA Networks integration",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json ./node_modules/.bin/nyc mocha -r ts-node/register -r source-map-support/register tests/bootstrap.ts tests/**-specs.ts",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "./node_modules/.bin/tslint --project . --exclude ./node_modules/*",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearesho-team/cpa-integration-frontend.git"
  },
  "keywords": [
    "react",
    "form",
    "group",
    "typescript",
    "package",
    "forms",
    "inputs"
  ],
  "author": "Alexander Letnikow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wearesho-team/cpa-integration-frontend/issues"
  },
  "homepage": "https://github.com/wearesho-team/cpa-integration-frontend#readme",
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/chai-enzyme": "^0.6.2",
    "@types/mocha": "^2.2.41",
    "awesome-typescript-loader": "^3.2.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.1",
    "circular-dependency-plugin": "^4.2.0",
    "clean-webpack-plugin": "^0.1.16",
    "codecov": "^2.3.0",
    "cross-env": "^5.0.5",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "pre-commit": "^1.2.2",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {},
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude:": [
      "**/*.d.ts",
      "webpack.config.js",
      "codecept.conf.js",
      "tests"
    ],
    "reporter": [
      "clover",
      "text-summary"
    ],
    "report-dir": "./tests/output"
  },
  "dependencies": {
    "url-search-params-polyfill": "^2.0.1"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
